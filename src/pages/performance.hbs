            document.addEventListener('DOMContentLoaded', () => {
                const elements = Array.from(document.querySelectorAll('.performance-services'));
                elements.forEach(element => {
                    const container = element.querySelector('.swiper')
                    new Swiper(container, {
                        slidesPerView: 1,
                        spaceBetween: 40,
                        speed: 600,
                        navigation: {
                            prevEl: element.querySelector('.performance-services__slider-arrow--prev'),
                            nextEl: element.querySelector('.performance-services__slider-arrow--next')
                        },
                        breakpoints: {
                            641: {
                                slidesPerView: 2,
                                spaceBetween: 20,
                            }
                        }
                    })
                })
            })

        </script>


    <section class="intro js-scroll-container">
      <div class="container">
        <div class="intro__content">
          <div class="intro__column">
            <h1 class="intro__title js-scroll-item">
              <span class="orange">Федеральная</span>
              партнерская сеть
            </h1>
            <p class="intro__text js-scroll-item">рекламных конструкций</p>
            <ul class="intro__list js-scroll-item">
              <li class="intro__info">
                <div class="intro__info-icon-wrapper">
                  <svg width="12" height="12" aria-hidden="true" class="icon-down">
                    <use xlink:href="#checkmark-white"></use>
